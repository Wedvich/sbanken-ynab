  "main": "server.js",
  "scripts": {
    "build": "git clean -xdf wwwroot && webpack --config webpack.config.prod.js",
    "dev": "webpack serve --config webpack.config.dev.js",
    "lint": "eslint . --fix --report-unused-disable-directives",
    "lint-ci": "eslint .",
    "lint-staged": "lint-staged",
    "mock-api": "dotnet run -p mock-api/mock-api.csproj",
    "prepare": "husky install && yarn snyk-protect",
    "prettier": "prettier --write 'src/**/*.{js,ts,tsx,json,scss}'",
    "snyk-protect": "snyk protect",
    "start": "dotnet run",
    "stylelint": "stylelint src/**/*.css --fix",
    "test": "jest --coverage --passWithNoTests",
    "test-ci": "jest --ci --coverage --passWithNoTests",
    "typecheck": "tsc"
  },
  "snyk": true,
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "core-js": "^3.12.1",
